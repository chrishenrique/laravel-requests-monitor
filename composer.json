{
    "name": "chrishenrique/laravel-requests-monitor",
    "description": "Centraliza requisições de apps Laravel em um DB unificado via job e middleware.",
    "type": "laravel-package",
    "keywords": ["laravel", "requests", "monitor", "logging", "queue"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christiano Costa",
            "email": "chrishenrique16@hotmail.com",
            "homepage": "https://christianocosta.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
       
    },
    "autoload": {
        "psr-4": {
            "ChrisHenrique\\RequestsMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
           
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisHenrique\\RequestsMonitor\\RequestsMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
